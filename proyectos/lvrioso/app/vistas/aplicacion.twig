
    {% extends "plantillas/base.twig" %}

    {% block titulo %}Aplicacion de Repositorio{% endblock %}

    {% block stylesheets %}
            {{ parent() }} 
          <link rel="stylesheet" href="{{ constant('APP_URL') }}assets/css/aplicacion.css">
    {% endblock %}
    
    

{% block contenido %}
    <div class="contenedor">
        <header class="header">
            <nav class="nav">
                <span class="nav-toogle icono-grande" id="btn-nav-toggle">
                    <i class="fe fe-bar"></i>
                </span>
                <span class="menu-open icono-grande" id="btn-menu-open">
                    <i class="fe fe-list-bullet"></i>
                    <!--iconos-->
                </span>
                <!-- se puede poner varias nombres de clase-->
                <ul class="nav-links lista-links" id="nav-links">
                    <li> <a href="{{ path_for ('inicio') }}">Inicio</a></li>
                    <li> <a href="https://www.pexels.com/search/cat/">Contenido</a></li>
                    <li> <a href="#">Nueva actividad</a></li>
                </ul>
            </nav>
        </header>
        <div class="menu" id="menu-lateral" data-url="{{ path_for('aplicacion.actividades') }}">
            <span class="menu-close" id="btn-menu-close">
                 <i class="fe fe-close"></i>
            </span>
            <ul class="lista-links" id="menu-links">
                <li><a href="#">UNAL</a></li>
                <li><a href="#">GAIA</a></li>
                <li><a href="https://www.pexels.com/search/whale/">Ballenas</a></li>
                <li><a href="https://www.pexels.com/search/cars/">Carros</a></li>
                <li><a href="https://www.pexels.com/search/skyscrapers/">Edificios</a></li>
            </ul>
        </div>
        <section class="main">
            <div class="instruccion">
                <p>Hello Is Party Time!</p>
            </div>
            <div class="contenedor-iframe" >
                <iframe id="iframe" name="main-iframe" src="https://www.angular.io/" frameborder="0"></iframe>
            </div>
        </section>
          {% include "fragmentos/footer.twig" %}
    </div>

   
    

{% endblock %}

{% block javascripts %}
{{ parent() }}
<script src="{{ constant('APP_URL') }}assets/js/aplicacion.js"></script>
{% endblock %} 
